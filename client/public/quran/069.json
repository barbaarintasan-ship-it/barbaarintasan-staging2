{
  "number": 69,
  "name": "سُورَةُ الحَاقَّةِ",
  "englishName": "Al-Haaqqa",
  "englishNameTranslation": "The Reality",
  "revelationType": "Meccan",
  "numberOfAyahs": 52,
  "ayahs": [
    {
      "number": 1,
      "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ ٱلۡحَاۤقَّةُ",
      "juz": 29,
      "page": 566
    },
    {
      "number": 2,
      "text": "مَا ٱلۡحَاۤقَّةُ",
      "juz": 29,
      "page": 566
    },
    {
      "number": 3,
      "text": "وَمَاۤ أَدۡرَىٰكَ مَا ٱلۡحَاۤقَّةُ",
      "juz": 29,
      "page": 566
    },
    {
      "number": 4,
      "text": "كَذَّبَتۡ ثَمُودُ وَعَادُۢ بِٱلۡقَارِعَةِ",
      "juz": 29,
      "page": 566
    },
    {
      "number": 5,
      "text": "فَأَمَّا ثَمُودُ فَأُهۡلِكُوا۟ بِٱلطَّاغِیَةِ",
      "juz": 29,
      "page": 566
    },
    {
      "number": 6,
      "text": "وَأَمَّا عَادࣱ فَأُهۡلِكُوا۟ بِرِیحࣲ صَرۡصَرٍ عَاتِیَةࣲ",
      "juz": 29,
      "page": 566
    },
    {
      "number": 7,
      "text": "سَخَّرَهَا عَلَیۡهِمۡ سَبۡعَ لَیَالࣲ وَثَمَـٰنِیَةَ أَیَّامٍ حُسُومࣰاۖ فَتَرَى ٱلۡقَوۡمَ فِیهَا صَرۡعَىٰ كَأَنَّهُمۡ أَعۡجَازُ نَخۡلٍ خَاوِیَةࣲ",
      "juz": 29,
      "page": 566
    },
    {
      "number": 8,
      "text": "فَهَلۡ تَرَىٰ لَهُم مِّنۢ بَاقِیَةࣲ",
      "juz": 29,
      "page": 566
    },
    {
      "number": 9,
      "text": "وَجَاۤءَ فِرۡعَوۡنُ وَمَن قَبۡلَهُۥ وَٱلۡمُؤۡتَفِكَـٰتُ بِٱلۡخَاطِئَةِ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 10,
      "text": "فَعَصَوۡا۟ رَسُولَ رَبِّهِمۡ فَأَخَذَهُمۡ أَخۡذَةࣰ رَّابِیَةً",
      "juz": 29,
      "page": 567
    },
    {
      "number": 11,
      "text": "إِنَّا لَمَّا طَغَا ٱلۡمَاۤءُ حَمَلۡنَـٰكُمۡ فِی ٱلۡجَارِیَةِ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 12,
      "text": "لِنَجۡعَلَهَا لَكُمۡ تَذۡكِرَةࣰ وَتَعِیَهَاۤ أُذُنࣱ وَ ٰ⁠عِیَةࣱ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 13,
      "text": "فَإِذَا نُفِخَ فِی ٱلصُّورِ نَفۡخَةࣱ وَ ٰ⁠حِدَةࣱ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 14,
      "text": "وَحُمِلَتِ ٱلۡأَرۡضُ وَٱلۡجِبَالُ فَدُكَّتَا دَكَّةࣰ وَ ٰ⁠حِدَةࣰ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 15,
      "text": "فَیَوۡمَىِٕذࣲ وَقَعَتِ ٱلۡوَاقِعَةُ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 16,
      "text": "وَٱنشَقَّتِ ٱلسَّمَاۤءُ فَهِیَ یَوۡمَىِٕذࣲ وَاهِیَةࣱ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 17,
      "text": "وَٱلۡمَلَكُ عَلَىٰۤ أَرۡجَاۤىِٕهَاۚ وَیَحۡمِلُ عَرۡشَ رَبِّكَ فَوۡقَهُمۡ یَوۡمَىِٕذࣲ ثَمَـٰنِیَةࣱ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 18,
      "text": "یَوۡمَىِٕذࣲ تُعۡرَضُونَ لَا تَخۡفَىٰ مِنكُمۡ خَافِیَةࣱ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 19,
      "text": "فَأَمَّا مَنۡ أُوتِیَ كِتَـٰبَهُۥ بِیَمِینِهِۦ فَیَقُولُ هَاۤؤُمُ ٱقۡرَءُوا۟ كِتَـٰبِیَهۡ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 20,
      "text": "إِنِّی ظَنَنتُ أَنِّی مُلَـٰقٍ حِسَابِیَهۡ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 21,
      "text": "فَهُوَ فِی عِیشَةࣲ رَّاضِیَةࣲ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 22,
      "text": "فِی جَنَّةٍ عَالِیَةࣲ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 23,
      "text": "قُطُوفُهَا دَانِیَةࣱ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 24,
      "text": "كُلُوا۟ وَٱشۡرَبُوا۟ هَنِیۤـَٔۢا بِمَاۤ أَسۡلَفۡتُمۡ فِی ٱلۡأَیَّامِ ٱلۡخَالِیَةِ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 25,
      "text": "وَأَمَّا مَنۡ أُوتِیَ كِتَـٰبَهُۥ بِشِمَالِهِۦ فَیَقُولُ یَـٰلَیۡتَنِی لَمۡ أُوتَ كِتَـٰبِیَهۡ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 26,
      "text": "وَلَمۡ أَدۡرِ مَا حِسَابِیَهۡ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 27,
      "text": "یَـٰلَیۡتَهَا كَانَتِ ٱلۡقَاضِیَةَ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 28,
      "text": "مَاۤ أَغۡنَىٰ عَنِّی مَالِیَهۡۜ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 29,
      "text": "هَلَكَ عَنِّی سُلۡطَـٰنِیَهۡ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 30,
      "text": "خُذُوهُ فَغُلُّوهُ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 31,
      "text": "ثُمَّ ٱلۡجَحِیمَ صَلُّوهُ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 32,
      "text": "ثُمَّ فِی سِلۡسِلَةࣲ ذَرۡعُهَا سَبۡعُونَ ذِرَاعࣰا فَٱسۡلُكُوهُ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 33,
      "text": "إِنَّهُۥ كَانَ لَا یُؤۡمِنُ بِٱللَّهِ ٱلۡعَظِیمِ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 34,
      "text": "وَلَا یَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِینِ",
      "juz": 29,
      "page": 567
    },
    {
      "number": 35,
      "text": "فَلَیۡسَ لَهُ ٱلۡیَوۡمَ هَـٰهُنَا حَمِیمࣱ",
      "juz": 29,
      "page": 568
    },
    {
      "number": 36,
      "text": "وَلَا طَعَامٌ إِلَّا مِنۡ غِسۡلِینࣲ",
      "juz": 29,
      "page": 568
    },
    {
      "number": 37,
      "text": "لَّا یَأۡكُلُهُۥۤ إِلَّا ٱلۡخَـٰطِـُٔونَ",
      "juz": 29,
      "page": 568
    },
    {
      "number": 38,
      "text": "فَلَاۤ أُقۡسِمُ بِمَا تُبۡصِرُونَ",
      "juz": 29,
      "page": 568
    },
    {
      "number": 39,
      "text": "وَمَا لَا تُبۡصِرُونَ",
      "juz": 29,
      "page": 568
    },
    {
      "number": 40,
      "text": "إِنَّهُۥ لَقَوۡلُ رَسُولࣲ كَرِیمࣲ",
      "juz": 29,
      "page": 568
    },
    {
      "number": 41,
      "text": "وَمَا هُوَ بِقَوۡلِ شَاعِرࣲۚ قَلِیلࣰا مَّا تُؤۡمِنُونَ",
      "juz": 29,
      "page": 568
    },
    {
      "number": 42,
      "text": "وَلَا بِقَوۡلِ كَاهِنࣲۚ قَلِیلࣰا مَّا تَذَكَّرُونَ",
      "juz": 29,
      "page": 568
    },
    {
      "number": 43,
      "text": "تَنزِیلࣱ مِّن رَّبِّ ٱلۡعَـٰلَمِینَ",
      "juz": 29,
      "page": 568
    },
    {
      "number": 44,
      "text": "وَلَوۡ تَقَوَّلَ عَلَیۡنَا بَعۡضَ ٱلۡأَقَاوِیلِ",
      "juz": 29,
      "page": 568
    },
    {
      "number": 45,
      "text": "لَأَخَذۡنَا مِنۡهُ بِٱلۡیَمِینِ",
      "juz": 29,
      "page": 568
    },
    {
      "number": 46,
      "text": "ثُمَّ لَقَطَعۡنَا مِنۡهُ ٱلۡوَتِینَ",
      "juz": 29,
      "page": 568
    },
    {
      "number": 47,
      "text": "فَمَا مِنكُم مِّنۡ أَحَدٍ عَنۡهُ حَـٰجِزِینَ",
      "juz": 29,
      "page": 568
    },
    {
      "number": 48,
      "text": "وَإِنَّهُۥ لَتَذۡكِرَةࣱ لِّلۡمُتَّقِینَ",
      "juz": 29,
      "page": 568
    },
    {
      "number": 49,
      "text": "وَإِنَّا لَنَعۡلَمُ أَنَّ مِنكُم مُّكَذِّبِینَ",
      "juz": 29,
      "page": 568
    },
    {
      "number": 50,
      "text": "وَإِنَّهُۥ لَحَسۡرَةٌ عَلَى ٱلۡكَـٰفِرِینَ",
      "juz": 29,
      "page": 568
    },
    {
      "number": 51,
      "text": "وَإِنَّهُۥ لَحَقُّ ٱلۡیَقِینِ",
      "juz": 29,
      "page": 568
    },
    {
      "number": 52,
      "text": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِیمِ",
      "juz": 29,
      "page": 568
    }
  ]
}