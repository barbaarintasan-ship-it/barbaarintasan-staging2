{
  "number": 80,
  "name": "سُورَةُ عَبَسَ",
  "englishName": "Abasa",
  "englishNameTranslation": "He frowned",
  "revelationType": "Meccan",
  "numberOfAyahs": 42,
  "ayahs": [
    {
      "number": 1,
      "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ عَبَسَ وَتَوَلَّىٰۤ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 2,
      "text": "أَن جَاۤءَهُ ٱلۡأَعۡمَىٰ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 3,
      "text": "وَمَا یُدۡرِیكَ لَعَلَّهُۥ یَزَّكَّىٰۤ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 4,
      "text": "أَوۡ یَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكۡرَىٰۤ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 5,
      "text": "أَمَّا مَنِ ٱسۡتَغۡنَىٰ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 6,
      "text": "فَأَنتَ لَهُۥ تَصَدَّىٰ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 7,
      "text": "وَمَا عَلَیۡكَ أَلَّا یَزَّكَّىٰ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 8,
      "text": "وَأَمَّا مَن جَاۤءَكَ یَسۡعَىٰ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 9,
      "text": "وَهُوَ یَخۡشَىٰ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 10,
      "text": "فَأَنتَ عَنۡهُ تَلَهَّىٰ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 11,
      "text": "كَلَّاۤ إِنَّهَا تَذۡكِرَةࣱ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 12,
      "text": "فَمَن شَاۤءَ ذَكَرَهُۥ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 13,
      "text": "فِی صُحُفࣲ مُّكَرَّمَةࣲ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 14,
      "text": "مَّرۡفُوعَةࣲ مُّطَهَّرَةِۭ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 15,
      "text": "بِأَیۡدِی سَفَرَةࣲ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 16,
      "text": "كِرَامِۭ بَرَرَةࣲ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 17,
      "text": "قُتِلَ ٱلۡإِنسَـٰنُ مَاۤ أَكۡفَرَهُۥ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 18,
      "text": "مِنۡ أَیِّ شَیۡءٍ خَلَقَهُۥ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 19,
      "text": "مِن نُّطۡفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 20,
      "text": "ثُمَّ ٱلسَّبِیلَ یَسَّرَهُۥ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 21,
      "text": "ثُمَّ أَمَاتَهُۥ فَأَقۡبَرَهُۥ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 22,
      "text": "ثُمَّ إِذَا شَاۤءَ أَنشَرَهُۥ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 23,
      "text": "كَلَّا لَمَّا یَقۡضِ مَاۤ أَمَرَهُۥ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 24,
      "text": "فَلۡیَنظُرِ ٱلۡإِنسَـٰنُ إِلَىٰ طَعَامِهِۦۤ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 25,
      "text": "أَنَّا صَبَبۡنَا ٱلۡمَاۤءَ صَبࣰّا",
      "juz": 30,
      "page": 585
    },
    {
      "number": 26,
      "text": "ثُمَّ شَقَقۡنَا ٱلۡأَرۡضَ شَقࣰّا",
      "juz": 30,
      "page": 585
    },
    {
      "number": 27,
      "text": "فَأَنۢبَتۡنَا فِیهَا حَبࣰّا",
      "juz": 30,
      "page": 585
    },
    {
      "number": 28,
      "text": "وَعِنَبࣰا وَقَضۡبࣰا",
      "juz": 30,
      "page": 585
    },
    {
      "number": 29,
      "text": "وَزَیۡتُونࣰا وَنَخۡلࣰا",
      "juz": 30,
      "page": 585
    },
    {
      "number": 30,
      "text": "وَحَدَاۤىِٕقَ غُلۡبࣰا",
      "juz": 30,
      "page": 585
    },
    {
      "number": 31,
      "text": "وَفَـٰكِهَةࣰ وَأَبࣰّا",
      "juz": 30,
      "page": 585
    },
    {
      "number": 32,
      "text": "مَّتَـٰعࣰا لَّكُمۡ وَلِأَنۡعَـٰمِكُمۡ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 33,
      "text": "فَإِذَا جَاۤءَتِ ٱلصَّاۤخَّةُ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 34,
      "text": "یَوۡمَ یَفِرُّ ٱلۡمَرۡءُ مِنۡ أَخِیهِ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 35,
      "text": "وَأُمِّهِۦ وَأَبِیهِ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 36,
      "text": "وَصَـٰحِبَتِهِۦ وَبَنِیهِ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 37,
      "text": "لِكُلِّ ٱمۡرِئࣲ مِّنۡهُمۡ یَوۡمَىِٕذࣲ شَأۡنࣱ یُغۡنِیهِ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 38,
      "text": "وُجُوهࣱ یَوۡمَىِٕذࣲ مُّسۡفِرَةࣱ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 39,
      "text": "ضَاحِكَةࣱ مُّسۡتَبۡشِرَةࣱ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 40,
      "text": "وَوُجُوهࣱ یَوۡمَىِٕذٍ عَلَیۡهَا غَبَرَةࣱ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 41,
      "text": "تَرۡهَقُهَا قَتَرَةٌ",
      "juz": 30,
      "page": 585
    },
    {
      "number": 42,
      "text": "أُو۟لَـٰۤىِٕكَ هُمُ ٱلۡكَفَرَةُ ٱلۡفَجَرَةُ",
      "juz": 30,
      "page": 585
    }
  ]
}