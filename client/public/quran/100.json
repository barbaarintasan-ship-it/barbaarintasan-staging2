{
  "number": 100,
  "name": "سُورَةُ العَادِيَاتِ",
  "englishName": "Al-Aadiyaat",
  "englishNameTranslation": "The Chargers",
  "revelationType": "Meccan",
  "numberOfAyahs": 11,
  "ayahs": [
    {
      "number": 1,
      "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ وَٱلۡعَـٰدِیَـٰتِ ضَبۡحࣰا",
      "juz": 30,
      "page": 599
    },
    {
      "number": 2,
      "text": "فَٱلۡمُورِیَـٰتِ قَدۡحࣰا",
      "juz": 30,
      "page": 599
    },
    {
      "number": 3,
      "text": "فَٱلۡمُغِیرَ ٰ⁠تِ صُبۡحࣰا",
      "juz": 30,
      "page": 599
    },
    {
      "number": 4,
      "text": "فَأَثَرۡنَ بِهِۦ نَقۡعࣰا",
      "juz": 30,
      "page": 599
    },
    {
      "number": 5,
      "text": "فَوَسَطۡنَ بِهِۦ جَمۡعًا",
      "juz": 30,
      "page": 599
    },
    {
      "number": 6,
      "text": "إِنَّ ٱلۡإِنسَـٰنَ لِرَبِّهِۦ لَكَنُودࣱ",
      "juz": 30,
      "page": 599
    },
    {
      "number": 7,
      "text": "وَإِنَّهُۥ عَلَىٰ ذَ ٰ⁠لِكَ لَشَهِیدࣱ",
      "juz": 30,
      "page": 599
    },
    {
      "number": 8,
      "text": "وَإِنَّهُۥ لِحُبِّ ٱلۡخَیۡرِ لَشَدِیدٌ",
      "juz": 30,
      "page": 599
    },
    {
      "number": 9,
      "text": "۞ أَفَلَا یَعۡلَمُ إِذَا بُعۡثِرَ مَا فِی ٱلۡقُبُورِ",
      "juz": 30,
      "page": 599
    },
    {
      "number": 10,
      "text": "وَحُصِّلَ مَا فِی ٱلصُّدُورِ",
      "juz": 30,
      "page": 600
    },
    {
      "number": 11,
      "text": "إِنَّ رَبَّهُم بِهِمۡ یَوۡمَىِٕذࣲ لَّخَبِیرُۢ",
      "juz": 30,
      "page": 600
    }
  ]
}