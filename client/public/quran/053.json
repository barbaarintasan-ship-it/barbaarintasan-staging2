{
  "number": 53,
  "name": "سُورَةُ النَّجۡمِ",
  "englishName": "An-Najm",
  "englishNameTranslation": "The Star",
  "revelationType": "Meccan",
  "numberOfAyahs": 62,
  "ayahs": [
    {
      "number": 1,
      "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ وَٱلنَّجۡمِ إِذَا هَوَىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 2,
      "text": "مَا ضَلَّ صَاحِبُكُمۡ وَمَا غَوَىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 3,
      "text": "وَمَا یَنطِقُ عَنِ ٱلۡهَوَىٰۤ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 4,
      "text": "إِنۡ هُوَ إِلَّا وَحۡیࣱ یُوحَىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 5,
      "text": "عَلَّمَهُۥ شَدِیدُ ٱلۡقُوَىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 6,
      "text": "ذُو مِرَّةࣲ فَٱسۡتَوَىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 7,
      "text": "وَهُوَ بِٱلۡأُفُقِ ٱلۡأَعۡلَىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 8,
      "text": "ثُمَّ دَنَا فَتَدَلَّىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 9,
      "text": "فَكَانَ قَابَ قَوۡسَیۡنِ أَوۡ أَدۡنَىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 10,
      "text": "فَأَوۡحَىٰۤ إِلَىٰ عَبۡدِهِۦ مَاۤ أَوۡحَىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 11,
      "text": "مَا كَذَبَ ٱلۡفُؤَادُ مَا رَأَىٰۤ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 12,
      "text": "أَفَتُمَـٰرُونَهُۥ عَلَىٰ مَا یَرَىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 13,
      "text": "وَلَقَدۡ رَءَاهُ نَزۡلَةً أُخۡرَىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 14,
      "text": "عِندَ سِدۡرَةِ ٱلۡمُنتَهَىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 15,
      "text": "عِندَهَا جَنَّةُ ٱلۡمَأۡوَىٰۤ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 16,
      "text": "إِذۡ یَغۡشَى ٱلسِّدۡرَةَ مَا یَغۡشَىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 17,
      "text": "مَا زَاغَ ٱلۡبَصَرُ وَمَا طَغَىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 18,
      "text": "لَقَدۡ رَأَىٰ مِنۡ ءَایَـٰتِ رَبِّهِ ٱلۡكُبۡرَىٰۤ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 19,
      "text": "أَفَرَءَیۡتُمُ ٱللَّـٰتَ وَٱلۡعُزَّىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 20,
      "text": "وَمَنَوٰةَ ٱلثَّالِثَةَ ٱلۡأُخۡرَىٰۤ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 21,
      "text": "أَلَكُمُ ٱلذَّكَرُ وَلَهُ ٱلۡأُنثَىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 22,
      "text": "تِلۡكَ إِذࣰا قِسۡمَةࣱ ضِیزَىٰۤ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 23,
      "text": "إِنۡ هِیَ إِلَّاۤ أَسۡمَاۤءࣱ سَمَّیۡتُمُوهَاۤ أَنتُمۡ وَءَابَاۤؤُكُم مَّاۤ أَنزَلَ ٱللَّهُ بِهَا مِن سُلۡطَـٰنٍۚ إِن یَتَّبِعُونَ إِلَّا ٱلظَّنَّ وَمَا تَهۡوَى ٱلۡأَنفُسُۖ وَلَقَدۡ جَاۤءَهُم مِّن رَّبِّهِمُ ٱلۡهُدَىٰۤ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 24,
      "text": "أَمۡ لِلۡإِنسَـٰنِ مَا تَمَنَّىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 25,
      "text": "فَلِلَّهِ ٱلۡـَٔاخِرَةُ وَٱلۡأُولَىٰ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 26,
      "text": "۞ وَكَم مِّن مَّلَكࣲ فِی ٱلسَّمَـٰوَ ٰ⁠تِ لَا تُغۡنِی شَفَـٰعَتُهُمۡ شَیۡـًٔا إِلَّا مِنۢ بَعۡدِ أَن یَأۡذَنَ ٱللَّهُ لِمَن یَشَاۤءُ وَیَرۡضَىٰۤ",
      "juz": 27,
      "page": 526
    },
    {
      "number": 27,
      "text": "إِنَّ ٱلَّذِینَ لَا یُؤۡمِنُونَ بِٱلۡـَٔاخِرَةِ لَیُسَمُّونَ ٱلۡمَلَـٰۤىِٕكَةَ تَسۡمِیَةَ ٱلۡأُنثَىٰ",
      "juz": 27,
      "page": 527
    },
    {
      "number": 28,
      "text": "وَمَا لَهُم بِهِۦ مِنۡ عِلۡمٍۖ إِن یَتَّبِعُونَ إِلَّا ٱلظَّنَّۖ وَإِنَّ ٱلظَّنَّ لَا یُغۡنِی مِنَ ٱلۡحَقِّ شَیۡـࣰٔا",
      "juz": 27,
      "page": 527
    },
    {
      "number": 29,
      "text": "فَأَعۡرِضۡ عَن مَّن تَوَلَّىٰ عَن ذِكۡرِنَا وَلَمۡ یُرِدۡ إِلَّا ٱلۡحَیَوٰةَ ٱلدُّنۡیَا",
      "juz": 27,
      "page": 527
    },
    {
      "number": 30,
      "text": "ذَ ٰ⁠لِكَ مَبۡلَغُهُم مِّنَ ٱلۡعِلۡمِۚ إِنَّ رَبَّكَ هُوَ أَعۡلَمُ بِمَن ضَلَّ عَن سَبِیلِهِۦ وَهُوَ أَعۡلَمُ بِمَنِ ٱهۡتَدَىٰ",
      "juz": 27,
      "page": 527
    },
    {
      "number": 31,
      "text": "وَلِلَّهِ مَا فِی ٱلسَّمَـٰوَ ٰ⁠تِ وَمَا فِی ٱلۡأَرۡضِ لِیَجۡزِیَ ٱلَّذِینَ أَسَـٰۤـُٔوا۟ بِمَا عَمِلُوا۟ وَیَجۡزِیَ ٱلَّذِینَ أَحۡسَنُوا۟ بِٱلۡحُسۡنَى",
      "juz": 27,
      "page": 527
    },
    {
      "number": 32,
      "text": "ٱلَّذِینَ یَجۡتَنِبُونَ كَبَـٰۤىِٕرَ ٱلۡإِثۡمِ وَٱلۡفَوَ ٰ⁠حِشَ إِلَّا ٱللَّمَمَۚ إِنَّ رَبَّكَ وَ ٰ⁠سِعُ ٱلۡمَغۡفِرَةِۚ هُوَ أَعۡلَمُ بِكُمۡ إِذۡ أَنشَأَكُم مِّنَ ٱلۡأَرۡضِ وَإِذۡ أَنتُمۡ أَجِنَّةࣱ فِی بُطُونِ أُمَّهَـٰتِكُمۡۖ فَلَا تُزَكُّوۤا۟ أَنفُسَكُمۡۖ هُوَ أَعۡلَمُ بِمَنِ ٱتَّقَىٰۤ",
      "juz": 27,
      "page": 527
    },
    {
      "number": 33,
      "text": "أَفَرَءَیۡتَ ٱلَّذِی تَوَلَّىٰ",
      "juz": 27,
      "page": 527
    },
    {
      "number": 34,
      "text": "وَأَعۡطَىٰ قَلِیلࣰا وَأَكۡدَىٰۤ",
      "juz": 27,
      "page": 527
    },
    {
      "number": 35,
      "text": "أَعِندَهُۥ عِلۡمُ ٱلۡغَیۡبِ فَهُوَ یَرَىٰۤ",
      "juz": 27,
      "page": 527
    },
    {
      "number": 36,
      "text": "أَمۡ لَمۡ یُنَبَّأۡ بِمَا فِی صُحُفِ مُوسَىٰ",
      "juz": 27,
      "page": 527
    },
    {
      "number": 37,
      "text": "وَإِبۡرَ ٰ⁠هِیمَ ٱلَّذِی وَفَّىٰۤ",
      "juz": 27,
      "page": 527
    },
    {
      "number": 38,
      "text": "أَلَّا تَزِرُ وَازِرَةࣱ وِزۡرَ أُخۡرَىٰ",
      "juz": 27,
      "page": 527
    },
    {
      "number": 39,
      "text": "وَأَن لَّیۡسَ لِلۡإِنسَـٰنِ إِلَّا مَا سَعَىٰ",
      "juz": 27,
      "page": 527
    },
    {
      "number": 40,
      "text": "وَأَنَّ سَعۡیَهُۥ سَوۡفَ یُرَىٰ",
      "juz": 27,
      "page": 527
    },
    {
      "number": 41,
      "text": "ثُمَّ یُجۡزَىٰهُ ٱلۡجَزَاۤءَ ٱلۡأَوۡفَىٰ",
      "juz": 27,
      "page": 527
    },
    {
      "number": 42,
      "text": "وَأَنَّ إِلَىٰ رَبِّكَ ٱلۡمُنتَهَىٰ",
      "juz": 27,
      "page": 527
    },
    {
      "number": 43,
      "text": "وَأَنَّهُۥ هُوَ أَضۡحَكَ وَأَبۡكَىٰ",
      "juz": 27,
      "page": 527
    },
    {
      "number": 44,
      "text": "وَأَنَّهُۥ هُوَ أَمَاتَ وَأَحۡیَا",
      "juz": 27,
      "page": 527
    },
    {
      "number": 45,
      "text": "وَأَنَّهُۥ خَلَقَ ٱلزَّوۡجَیۡنِ ٱلذَّكَرَ وَٱلۡأُنثَىٰ",
      "juz": 27,
      "page": 528
    },
    {
      "number": 46,
      "text": "مِن نُّطۡفَةٍ إِذَا تُمۡنَىٰ",
      "juz": 27,
      "page": 528
    },
    {
      "number": 47,
      "text": "وَأَنَّ عَلَیۡهِ ٱلنَّشۡأَةَ ٱلۡأُخۡرَىٰ",
      "juz": 27,
      "page": 528
    },
    {
      "number": 48,
      "text": "وَأَنَّهُۥ هُوَ أَغۡنَىٰ وَأَقۡنَىٰ",
      "juz": 27,
      "page": 528
    },
    {
      "number": 49,
      "text": "وَأَنَّهُۥ هُوَ رَبُّ ٱلشِّعۡرَىٰ",
      "juz": 27,
      "page": 528
    },
    {
      "number": 50,
      "text": "وَأَنَّهُۥۤ أَهۡلَكَ عَادًا ٱلۡأُولَىٰ",
      "juz": 27,
      "page": 528
    },
    {
      "number": 51,
      "text": "وَثَمُودَا۟ فَمَاۤ أَبۡقَىٰ",
      "juz": 27,
      "page": 528
    },
    {
      "number": 52,
      "text": "وَقَوۡمَ نُوحࣲ مِّن قَبۡلُۖ إِنَّهُمۡ كَانُوا۟ هُمۡ أَظۡلَمَ وَأَطۡغَىٰ",
      "juz": 27,
      "page": 528
    },
    {
      "number": 53,
      "text": "وَٱلۡمُؤۡتَفِكَةَ أَهۡوَىٰ",
      "juz": 27,
      "page": 528
    },
    {
      "number": 54,
      "text": "فَغَشَّىٰهَا مَا غَشَّىٰ",
      "juz": 27,
      "page": 528
    },
    {
      "number": 55,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكَ تَتَمَارَىٰ",
      "juz": 27,
      "page": 528
    },
    {
      "number": 56,
      "text": "هَـٰذَا نَذِیرࣱ مِّنَ ٱلنُّذُرِ ٱلۡأُولَىٰۤ",
      "juz": 27,
      "page": 528
    },
    {
      "number": 57,
      "text": "أَزِفَتِ ٱلۡـَٔازِفَةُ",
      "juz": 27,
      "page": 528
    },
    {
      "number": 58,
      "text": "لَیۡسَ لَهَا مِن دُونِ ٱللَّهِ كَاشِفَةٌ",
      "juz": 27,
      "page": 528
    },
    {
      "number": 59,
      "text": "أَفَمِنۡ هَـٰذَا ٱلۡحَدِیثِ تَعۡجَبُونَ",
      "juz": 27,
      "page": 528
    },
    {
      "number": 60,
      "text": "وَتَضۡحَكُونَ وَلَا تَبۡكُونَ",
      "juz": 27,
      "page": 528
    },
    {
      "number": 61,
      "text": "وَأَنتُمۡ سَـٰمِدُونَ",
      "juz": 27,
      "page": 528
    },
    {
      "number": 62,
      "text": "فَٱسۡجُدُوا۟ لِلَّهِ وَٱعۡبُدُوا۟ ۩",
      "juz": 27,
      "page": 528
    }
  ]
}