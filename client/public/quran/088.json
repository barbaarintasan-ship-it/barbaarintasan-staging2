{
  "number": 88,
  "name": "سُورَةُ الغَاشِيَةِ",
  "englishName": "Al-Ghaashiya",
  "englishNameTranslation": "The Overwhelming",
  "revelationType": "Meccan",
  "numberOfAyahs": 26,
  "ayahs": [
    {
      "number": 1,
      "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ هَلۡ أَتَىٰكَ حَدِیثُ ٱلۡغَـٰشِیَةِ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 2,
      "text": "وُجُوهࣱ یَوۡمَىِٕذٍ خَـٰشِعَةٌ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 3,
      "text": "عَامِلَةࣱ نَّاصِبَةࣱ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 4,
      "text": "تَصۡلَىٰ نَارًا حَامِیَةࣰ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 5,
      "text": "تُسۡقَىٰ مِنۡ عَیۡنٍ ءَانِیَةࣲ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 6,
      "text": "لَّیۡسَ لَهُمۡ طَعَامٌ إِلَّا مِن ضَرِیعࣲ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 7,
      "text": "لَّا یُسۡمِنُ وَلَا یُغۡنِی مِن جُوعࣲ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 8,
      "text": "وُجُوهࣱ یَوۡمَىِٕذࣲ نَّاعِمَةࣱ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 9,
      "text": "لِّسَعۡیِهَا رَاضِیَةࣱ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 10,
      "text": "فِی جَنَّةٍ عَالِیَةࣲ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 11,
      "text": "لَّا تَسۡمَعُ فِیهَا لَـٰغِیَةࣰ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 12,
      "text": "فِیهَا عَیۡنࣱ جَارِیَةࣱ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 13,
      "text": "فِیهَا سُرُرࣱ مَّرۡفُوعَةࣱ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 14,
      "text": "وَأَكۡوَابࣱ مَّوۡضُوعَةࣱ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 15,
      "text": "وَنَمَارِقُ مَصۡفُوفَةࣱ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 16,
      "text": "وَزَرَابِیُّ مَبۡثُوثَةٌ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 17,
      "text": "أَفَلَا یَنظُرُونَ إِلَى ٱلۡإِبِلِ كَیۡفَ خُلِقَتۡ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 18,
      "text": "وَإِلَى ٱلسَّمَاۤءِ كَیۡفَ رُفِعَتۡ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 19,
      "text": "وَإِلَى ٱلۡجِبَالِ كَیۡفَ نُصِبَتۡ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 20,
      "text": "وَإِلَى ٱلۡأَرۡضِ كَیۡفَ سُطِحَتۡ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 21,
      "text": "فَذَكِّرۡ إِنَّمَاۤ أَنتَ مُذَكِّرࣱ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 22,
      "text": "لَّسۡتَ عَلَیۡهِم بِمُصَیۡطِرٍ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 23,
      "text": "إِلَّا مَن تَوَلَّىٰ وَكَفَرَ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 24,
      "text": "فَیُعَذِّبُهُ ٱللَّهُ ٱلۡعَذَابَ ٱلۡأَكۡبَرَ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 25,
      "text": "إِنَّ إِلَیۡنَاۤ إِیَابَهُمۡ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 26,
      "text": "ثُمَّ إِنَّ عَلَیۡنَا حِسَابَهُم",
      "juz": 30,
      "page": 592
    }
  ]
}