{
  "number": 85,
  "name": "سُورَةُ البُرُوجِ",
  "englishName": "Al-Burooj",
  "englishNameTranslation": "The Constellations",
  "revelationType": "Meccan",
  "numberOfAyahs": 22,
  "ayahs": [
    {
      "number": 1,
      "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ وَٱلسَّمَاۤءِ ذَاتِ ٱلۡبُرُوجِ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 2,
      "text": "وَٱلۡیَوۡمِ ٱلۡمَوۡعُودِ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 3,
      "text": "وَشَاهِدࣲ وَمَشۡهُودࣲ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 4,
      "text": "قُتِلَ أَصۡحَـٰبُ ٱلۡأُخۡدُودِ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 5,
      "text": "ٱلنَّارِ ذَاتِ ٱلۡوَقُودِ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 6,
      "text": "إِذۡ هُمۡ عَلَیۡهَا قُعُودࣱ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 7,
      "text": "وَهُمۡ عَلَىٰ مَا یَفۡعَلُونَ بِٱلۡمُؤۡمِنِینَ شُهُودࣱ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 8,
      "text": "وَمَا نَقَمُوا۟ مِنۡهُمۡ إِلَّاۤ أَن یُؤۡمِنُوا۟ بِٱللَّهِ ٱلۡعَزِیزِ ٱلۡحَمِیدِ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 9,
      "text": "ٱلَّذِی لَهُۥ مُلۡكُ ٱلسَّمَـٰوَ ٰ⁠تِ وَٱلۡأَرۡضِۚ وَٱللَّهُ عَلَىٰ كُلِّ شَیۡءࣲ شَهِیدٌ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 10,
      "text": "إِنَّ ٱلَّذِینَ فَتَنُوا۟ ٱلۡمُؤۡمِنِینَ وَٱلۡمُؤۡمِنَـٰتِ ثُمَّ لَمۡ یَتُوبُوا۟ فَلَهُمۡ عَذَابُ جَهَنَّمَ وَلَهُمۡ عَذَابُ ٱلۡحَرِیقِ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 11,
      "text": "إِنَّ ٱلَّذِینَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ لَهُمۡ جَنَّـٰتࣱ تَجۡرِی مِن تَحۡتِهَا ٱلۡأَنۡهَـٰرُۚ ذَ ٰ⁠لِكَ ٱلۡفَوۡزُ ٱلۡكَبِیرُ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 12,
      "text": "إِنَّ بَطۡشَ رَبِّكَ لَشَدِیدٌ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 13,
      "text": "إِنَّهُۥ هُوَ یُبۡدِئُ وَیُعِیدُ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 14,
      "text": "وَهُوَ ٱلۡغَفُورُ ٱلۡوَدُودُ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 15,
      "text": "ذُو ٱلۡعَرۡشِ ٱلۡمَجِیدُ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 16,
      "text": "فَعَّالࣱ لِّمَا یُرِیدُ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 17,
      "text": "هَلۡ أَتَىٰكَ حَدِیثُ ٱلۡجُنُودِ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 18,
      "text": "فِرۡعَوۡنَ وَثَمُودَ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 19,
      "text": "بَلِ ٱلَّذِینَ كَفَرُوا۟ فِی تَكۡذِیبࣲ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 20,
      "text": "وَٱللَّهُ مِن وَرَاۤىِٕهِم مُّحِیطُۢ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 21,
      "text": "بَلۡ هُوَ قُرۡءَانࣱ مَّجِیدࣱ",
      "juz": 30,
      "page": 590
    },
    {
      "number": 22,
      "text": "فِی لَوۡحࣲ مَّحۡفُوظِۭ",
      "juz": 30,
      "page": 590
    }
  ]
}