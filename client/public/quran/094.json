{
  "number": 94,
  "name": "سُورَةُ الشَّرۡحِ",
  "englishName": "Ash-Sharh",
  "englishNameTranslation": "The Consolation",
  "revelationType": "Meccan",
  "numberOfAyahs": 8,
  "ayahs": [
    {
      "number": 1,
      "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ أَلَمۡ نَشۡرَحۡ لَكَ صَدۡرَكَ",
      "juz": 30,
      "page": 596
    },
    {
      "number": 2,
      "text": "وَوَضَعۡنَا عَنكَ وِزۡرَكَ",
      "juz": 30,
      "page": 596
    },
    {
      "number": 3,
      "text": "ٱلَّذِیۤ أَنقَضَ ظَهۡرَكَ",
      "juz": 30,
      "page": 596
    },
    {
      "number": 4,
      "text": "وَرَفَعۡنَا لَكَ ذِكۡرَكَ",
      "juz": 30,
      "page": 596
    },
    {
      "number": 5,
      "text": "فَإِنَّ مَعَ ٱلۡعُسۡرِ یُسۡرًا",
      "juz": 30,
      "page": 596
    },
    {
      "number": 6,
      "text": "إِنَّ مَعَ ٱلۡعُسۡرِ یُسۡرࣰا",
      "juz": 30,
      "page": 596
    },
    {
      "number": 7,
      "text": "فَإِذَا فَرَغۡتَ فَٱنصَبۡ",
      "juz": 30,
      "page": 596
    },
    {
      "number": 8,
      "text": "وَإِلَىٰ رَبِّكَ فَٱرۡغَب",
      "juz": 30,
      "page": 596
    }
  ]
}