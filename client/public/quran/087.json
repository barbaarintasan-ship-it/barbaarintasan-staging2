{
  "number": 87,
  "name": "سُورَةُ الأَعۡلَىٰ",
  "englishName": "Al-A'laa",
  "englishNameTranslation": "The Most High",
  "revelationType": "Meccan",
  "numberOfAyahs": 19,
  "ayahs": [
    {
      "number": 1,
      "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ سَبِّحِ ٱسۡمَ رَبِّكَ ٱلۡأَعۡلَى",
      "juz": 30,
      "page": 591
    },
    {
      "number": 2,
      "text": "ٱلَّذِی خَلَقَ فَسَوَّىٰ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 3,
      "text": "وَٱلَّذِی قَدَّرَ فَهَدَىٰ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 4,
      "text": "وَٱلَّذِیۤ أَخۡرَجَ ٱلۡمَرۡعَىٰ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 5,
      "text": "فَجَعَلَهُۥ غُثَاۤءً أَحۡوَىٰ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 6,
      "text": "سَنُقۡرِئُكَ فَلَا تَنسَىٰۤ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 7,
      "text": "إِلَّا مَا شَاۤءَ ٱللَّهُۚ إِنَّهُۥ یَعۡلَمُ ٱلۡجَهۡرَ وَمَا یَخۡفَىٰ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 8,
      "text": "وَنُیَسِّرُكَ لِلۡیُسۡرَىٰ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 9,
      "text": "فَذَكِّرۡ إِن نَّفَعَتِ ٱلذِّكۡرَىٰ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 10,
      "text": "سَیَذَّكَّرُ مَن یَخۡشَىٰ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 11,
      "text": "وَیَتَجَنَّبُهَا ٱلۡأَشۡقَى",
      "juz": 30,
      "page": 591
    },
    {
      "number": 12,
      "text": "ٱلَّذِی یَصۡلَى ٱلنَّارَ ٱلۡكُبۡرَىٰ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 13,
      "text": "ثُمَّ لَا یَمُوتُ فِیهَا وَلَا یَحۡیَىٰ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 14,
      "text": "قَدۡ أَفۡلَحَ مَن تَزَكَّىٰ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 15,
      "text": "وَذَكَرَ ٱسۡمَ رَبِّهِۦ فَصَلَّىٰ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 16,
      "text": "بَلۡ تُؤۡثِرُونَ ٱلۡحَیَوٰةَ ٱلدُّنۡیَا",
      "juz": 30,
      "page": 592
    },
    {
      "number": 17,
      "text": "وَٱلۡـَٔاخِرَةُ خَیۡرࣱ وَأَبۡقَىٰۤ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 18,
      "text": "إِنَّ هَـٰذَا لَفِی ٱلصُّحُفِ ٱلۡأُولَىٰ",
      "juz": 30,
      "page": 592
    },
    {
      "number": 19,
      "text": "صُحُفِ إِبۡرَ ٰ⁠هِیمَ وَمُوسَىٰ",
      "juz": 30,
      "page": 592
    }
  ]
}