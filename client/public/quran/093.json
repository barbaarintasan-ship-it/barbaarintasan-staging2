{
  "number": 93,
  "name": "سُورَةُ الضُّحَىٰ",
  "englishName": "Ad-Dhuhaa",
  "englishNameTranslation": "The Morning Hours",
  "revelationType": "Meccan",
  "numberOfAyahs": 11,
  "ayahs": [
    {
      "number": 1,
      "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ وَٱلضُّحَىٰ",
      "juz": 30,
      "page": 596
    },
    {
      "number": 2,
      "text": "وَٱلَّیۡلِ إِذَا سَجَىٰ",
      "juz": 30,
      "page": 596
    },
    {
      "number": 3,
      "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ",
      "juz": 30,
      "page": 596
    },
    {
      "number": 4,
      "text": "وَلَلۡـَٔاخِرَةُ خَیۡرࣱ لَّكَ مِنَ ٱلۡأُولَىٰ",
      "juz": 30,
      "page": 596
    },
    {
      "number": 5,
      "text": "وَلَسَوۡفَ یُعۡطِیكَ رَبُّكَ فَتَرۡضَىٰۤ",
      "juz": 30,
      "page": 596
    },
    {
      "number": 6,
      "text": "أَلَمۡ یَجِدۡكَ یَتِیمࣰا فَـَٔاوَىٰ",
      "juz": 30,
      "page": 596
    },
    {
      "number": 7,
      "text": "وَوَجَدَكَ ضَاۤلࣰّا فَهَدَىٰ",
      "juz": 30,
      "page": 596
    },
    {
      "number": 8,
      "text": "وَوَجَدَكَ عَاۤىِٕلࣰا فَأَغۡنَىٰ",
      "juz": 30,
      "page": 596
    },
    {
      "number": 9,
      "text": "فَأَمَّا ٱلۡیَتِیمَ فَلَا تَقۡهَرۡ",
      "juz": 30,
      "page": 596
    },
    {
      "number": 10,
      "text": "وَأَمَّا ٱلسَّاۤىِٕلَ فَلَا تَنۡهَرۡ",
      "juz": 30,
      "page": 596
    },
    {
      "number": 11,
      "text": "وَأَمَّا بِنِعۡمَةِ رَبِّكَ فَحَدِّثۡ",
      "juz": 30,
      "page": 596
    }
  ]
}