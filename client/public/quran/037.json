{
  "number": 37,
  "name": "سُورَةُ الصَّافَّاتِ",
  "englishName": "As-Saaffaat",
  "englishNameTranslation": "Those drawn up in Ranks",
  "revelationType": "Meccan",
  "numberOfAyahs": 182,
  "ayahs": [
    {
      "number": 1,
      "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ وَٱلصَّـٰۤفَّـٰتِ صَفࣰّا",
      "juz": 23,
      "page": 446
    },
    {
      "number": 2,
      "text": "فَٱلزَّ ٰ⁠جِرَ ٰ⁠تِ زَجۡرࣰا",
      "juz": 23,
      "page": 446
    },
    {
      "number": 3,
      "text": "فَٱلتَّـٰلِیَـٰتِ ذِكۡرًا",
      "juz": 23,
      "page": 446
    },
    {
      "number": 4,
      "text": "إِنَّ إِلَـٰهَكُمۡ لَوَ ٰ⁠حِدࣱ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 5,
      "text": "رَّبُّ ٱلسَّمَـٰوَ ٰ⁠تِ وَٱلۡأَرۡضِ وَمَا بَیۡنَهُمَا وَرَبُّ ٱلۡمَشَـٰرِقِ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 6,
      "text": "إِنَّا زَیَّنَّا ٱلسَّمَاۤءَ ٱلدُّنۡیَا بِزِینَةٍ ٱلۡكَوَاكِبِ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 7,
      "text": "وَحِفۡظࣰا مِّن كُلِّ شَیۡطَـٰنࣲ مَّارِدࣲ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 8,
      "text": "لَّا یَسَّمَّعُونَ إِلَى ٱلۡمَلَإِ ٱلۡأَعۡلَىٰ وَیُقۡذَفُونَ مِن كُلِّ جَانِبࣲ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 9,
      "text": "دُحُورࣰاۖ وَلَهُمۡ عَذَابࣱ وَاصِبٌ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 10,
      "text": "إِلَّا مَنۡ خَطِفَ ٱلۡخَطۡفَةَ فَأَتۡبَعَهُۥ شِهَابࣱ ثَاقِبࣱ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 11,
      "text": "فَٱسۡتَفۡتِهِمۡ أَهُمۡ أَشَدُّ خَلۡقًا أَم مَّنۡ خَلَقۡنَاۤۚ إِنَّا خَلَقۡنَـٰهُم مِّن طِینࣲ لَّازِبِۭ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 12,
      "text": "بَلۡ عَجِبۡتَ وَیَسۡخَرُونَ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 13,
      "text": "وَإِذَا ذُكِّرُوا۟ لَا یَذۡكُرُونَ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 14,
      "text": "وَإِذَا رَأَوۡا۟ ءَایَةࣰ یَسۡتَسۡخِرُونَ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 15,
      "text": "وَقَالُوۤا۟ إِنۡ هَـٰذَاۤ إِلَّا سِحۡرࣱ مُّبِینٌ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 16,
      "text": "أَءِذَا مِتۡنَا وَكُنَّا تُرَابࣰا وَعِظَـٰمًا أَءِنَّا لَمَبۡعُوثُونَ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 17,
      "text": "أَوَءَابَاۤؤُنَا ٱلۡأَوَّلُونَ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 18,
      "text": "قُلۡ نَعَمۡ وَأَنتُمۡ دَ ٰ⁠خِرُونَ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 19,
      "text": "فَإِنَّمَا هِیَ زَجۡرَةࣱ وَ ٰ⁠حِدَةࣱ فَإِذَا هُمۡ یَنظُرُونَ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 20,
      "text": "وَقَالُوا۟ یَـٰوَیۡلَنَا هَـٰذَا یَوۡمُ ٱلدِّینِ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 21,
      "text": "هَـٰذَا یَوۡمُ ٱلۡفَصۡلِ ٱلَّذِی كُنتُم بِهِۦ تُكَذِّبُونَ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 22,
      "text": "۞ ٱحۡشُرُوا۟ ٱلَّذِینَ ظَلَمُوا۟ وَأَزۡوَ ٰ⁠جَهُمۡ وَمَا كَانُوا۟ یَعۡبُدُونَ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 23,
      "text": "مِن دُونِ ٱللَّهِ فَٱهۡدُوهُمۡ إِلَىٰ صِرَ ٰ⁠طِ ٱلۡجَحِیمِ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 24,
      "text": "وَقِفُوهُمۡۖ إِنَّهُم مَّسۡـُٔولُونَ",
      "juz": 23,
      "page": 446
    },
    {
      "number": 25,
      "text": "مَا لَكُمۡ لَا تَنَاصَرُونَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 26,
      "text": "بَلۡ هُمُ ٱلۡیَوۡمَ مُسۡتَسۡلِمُونَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 27,
      "text": "وَأَقۡبَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضࣲ یَتَسَاۤءَلُونَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 28,
      "text": "قَالُوۤا۟ إِنَّكُمۡ كُنتُمۡ تَأۡتُونَنَا عَنِ ٱلۡیَمِینِ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 29,
      "text": "قَالُوا۟ بَل لَّمۡ تَكُونُوا۟ مُؤۡمِنِینَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 30,
      "text": "وَمَا كَانَ لَنَا عَلَیۡكُم مِّن سُلۡطَـٰنِۭۖ بَلۡ كُنتُمۡ قَوۡمࣰا طَـٰغِینَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 31,
      "text": "فَحَقَّ عَلَیۡنَا قَوۡلُ رَبِّنَاۤۖ إِنَّا لَذَاۤىِٕقُونَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 32,
      "text": "فَأَغۡوَیۡنَـٰكُمۡ إِنَّا كُنَّا غَـٰوِینَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 33,
      "text": "فَإِنَّهُمۡ یَوۡمَىِٕذࣲ فِی ٱلۡعَذَابِ مُشۡتَرِكُونَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 34,
      "text": "إِنَّا كَذَ ٰ⁠لِكَ نَفۡعَلُ بِٱلۡمُجۡرِمِینَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 35,
      "text": "إِنَّهُمۡ كَانُوۤا۟ إِذَا قِیلَ لَهُمۡ لَاۤ إِلَـٰهَ إِلَّا ٱللَّهُ یَسۡتَكۡبِرُونَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 36,
      "text": "وَیَقُولُونَ أَىِٕنَّا لَتَارِكُوۤا۟ ءَالِهَتِنَا لِشَاعِرࣲ مَّجۡنُونِۭ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 37,
      "text": "بَلۡ جَاۤءَ بِٱلۡحَقِّ وَصَدَّقَ ٱلۡمُرۡسَلِینَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 38,
      "text": "إِنَّكُمۡ لَذَاۤىِٕقُوا۟ ٱلۡعَذَابِ ٱلۡأَلِیمِ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 39,
      "text": "وَمَا تُجۡزَوۡنَ إِلَّا مَا كُنتُمۡ تَعۡمَلُونَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 40,
      "text": "إِلَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِینَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 41,
      "text": "أُو۟لَـٰۤىِٕكَ لَهُمۡ رِزۡقࣱ مَّعۡلُومࣱ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 42,
      "text": "فَوَ ٰ⁠كِهُ وَهُم مُّكۡرَمُونَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 43,
      "text": "فِی جَنَّـٰتِ ٱلنَّعِیمِ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 44,
      "text": "عَلَىٰ سُرُرࣲ مُّتَقَـٰبِلِینَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 45,
      "text": "یُطَافُ عَلَیۡهِم بِكَأۡسࣲ مِّن مَّعِینِۭ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 46,
      "text": "بَیۡضَاۤءَ لَذَّةࣲ لِّلشَّـٰرِبِینَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 47,
      "text": "لَا فِیهَا غَوۡلࣱ وَلَا هُمۡ عَنۡهَا یُنزَفُونَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 48,
      "text": "وَعِندَهُمۡ قَـٰصِرَ ٰ⁠تُ ٱلطَّرۡفِ عِینࣱ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 49,
      "text": "كَأَنَّهُنَّ بَیۡضࣱ مَّكۡنُونࣱ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 50,
      "text": "فَأَقۡبَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضࣲ یَتَسَاۤءَلُونَ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 51,
      "text": "قَالَ قَاۤىِٕلࣱ مِّنۡهُمۡ إِنِّی كَانَ لِی قَرِینࣱ",
      "juz": 23,
      "page": 447
    },
    {
      "number": 52,
      "text": "یَقُولُ أَءِنَّكَ لَمِنَ ٱلۡمُصَدِّقِینَ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 53,
      "text": "أَءِذَا مِتۡنَا وَكُنَّا تُرَابࣰا وَعِظَـٰمًا أَءِنَّا لَمَدِینُونَ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 54,
      "text": "قَالَ هَلۡ أَنتُم مُّطَّلِعُونَ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 55,
      "text": "فَٱطَّلَعَ فَرَءَاهُ فِی سَوَاۤءِ ٱلۡجَحِیمِ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 56,
      "text": "قَالَ تَٱللَّهِ إِن كِدتَّ لَتُرۡدِینِ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 57,
      "text": "وَلَوۡلَا نِعۡمَةُ رَبِّی لَكُنتُ مِنَ ٱلۡمُحۡضَرِینَ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 58,
      "text": "أَفَمَا نَحۡنُ بِمَیِّتِینَ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 59,
      "text": "إِلَّا مَوۡتَتَنَا ٱلۡأُولَىٰ وَمَا نَحۡنُ بِمُعَذَّبِینَ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 60,
      "text": "إِنَّ هَـٰذَا لَهُوَ ٱلۡفَوۡزُ ٱلۡعَظِیمُ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 61,
      "text": "لِمِثۡلِ هَـٰذَا فَلۡیَعۡمَلِ ٱلۡعَـٰمِلُونَ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 62,
      "text": "أَذَ ٰ⁠لِكَ خَیۡرࣱ نُّزُلًا أَمۡ شَجَرَةُ ٱلزَّقُّومِ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 63,
      "text": "إِنَّا جَعَلۡنَـٰهَا فِتۡنَةࣰ لِّلظَّـٰلِمِینَ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 64,
      "text": "إِنَّهَا شَجَرَةࣱ تَخۡرُجُ فِیۤ أَصۡلِ ٱلۡجَحِیمِ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 65,
      "text": "طَلۡعُهَا كَأَنَّهُۥ رُءُوسُ ٱلشَّیَـٰطِینِ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 66,
      "text": "فَإِنَّهُمۡ لَـَٔاكِلُونَ مِنۡهَا فَمَالِـُٔونَ مِنۡهَا ٱلۡبُطُونَ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 67,
      "text": "ثُمَّ إِنَّ لَهُمۡ عَلَیۡهَا لَشَوۡبࣰا مِّنۡ حَمِیمࣲ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 68,
      "text": "ثُمَّ إِنَّ مَرۡجِعَهُمۡ لَإِلَى ٱلۡجَحِیمِ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 69,
      "text": "إِنَّهُمۡ أَلۡفَوۡا۟ ءَابَاۤءَهُمۡ ضَاۤلِّینَ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 70,
      "text": "فَهُمۡ عَلَىٰۤ ءَاثَـٰرِهِمۡ یُهۡرَعُونَ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 71,
      "text": "وَلَقَدۡ ضَلَّ قَبۡلَهُمۡ أَكۡثَرُ ٱلۡأَوَّلِینَ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 72,
      "text": "وَلَقَدۡ أَرۡسَلۡنَا فِیهِم مُّنذِرِینَ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 73,
      "text": "فَٱنظُرۡ كَیۡفَ كَانَ عَـٰقِبَةُ ٱلۡمُنذَرِینَ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 74,
      "text": "إِلَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِینَ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 75,
      "text": "وَلَقَدۡ نَادَىٰنَا نُوحࣱ فَلَنِعۡمَ ٱلۡمُجِیبُونَ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 76,
      "text": "وَنَجَّیۡنَـٰهُ وَأَهۡلَهُۥ مِنَ ٱلۡكَرۡبِ ٱلۡعَظِیمِ",
      "juz": 23,
      "page": 448
    },
    {
      "number": 77,
      "text": "وَجَعَلۡنَا ذُرِّیَّتَهُۥ هُمُ ٱلۡبَاقِینَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 78,
      "text": "وَتَرَكۡنَا عَلَیۡهِ فِی ٱلۡـَٔاخِرِینَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 79,
      "text": "سَلَـٰمٌ عَلَىٰ نُوحࣲ فِی ٱلۡعَـٰلَمِینَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 80,
      "text": "إِنَّا كَذَ ٰ⁠لِكَ نَجۡزِی ٱلۡمُحۡسِنِینَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 81,
      "text": "إِنَّهُۥ مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِینَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 82,
      "text": "ثُمَّ أَغۡرَقۡنَا ٱلۡـَٔاخَرِینَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 83,
      "text": "۞ وَإِنَّ مِن شِیعَتِهِۦ لَإِبۡرَ ٰ⁠هِیمَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 84,
      "text": "إِذۡ جَاۤءَ رَبَّهُۥ بِقَلۡبࣲ سَلِیمٍ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 85,
      "text": "إِذۡ قَالَ لِأَبِیهِ وَقَوۡمِهِۦ مَاذَا تَعۡبُدُونَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 86,
      "text": "أَىِٕفۡكًا ءَالِهَةࣰ دُونَ ٱللَّهِ تُرِیدُونَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 87,
      "text": "فَمَا ظَنُّكُم بِرَبِّ ٱلۡعَـٰلَمِینَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 88,
      "text": "فَنَظَرَ نَظۡرَةࣰ فِی ٱلنُّجُومِ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 89,
      "text": "فَقَالَ إِنِّی سَقِیمࣱ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 90,
      "text": "فَتَوَلَّوۡا۟ عَنۡهُ مُدۡبِرِینَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 91,
      "text": "فَرَاغَ إِلَىٰۤ ءَالِهَتِهِمۡ فَقَالَ أَلَا تَأۡكُلُونَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 92,
      "text": "مَا لَكُمۡ لَا تَنطِقُونَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 93,
      "text": "فَرَاغَ عَلَیۡهِمۡ ضَرۡبَۢا بِٱلۡیَمِینِ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 94,
      "text": "فَأَقۡبَلُوۤا۟ إِلَیۡهِ یَزِفُّونَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 95,
      "text": "قَالَ أَتَعۡبُدُونَ مَا تَنۡحِتُونَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 96,
      "text": "وَٱللَّهُ خَلَقَكُمۡ وَمَا تَعۡمَلُونَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 97,
      "text": "قَالُوا۟ ٱبۡنُوا۟ لَهُۥ بُنۡیَـٰنࣰا فَأَلۡقُوهُ فِی ٱلۡجَحِیمِ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 98,
      "text": "فَأَرَادُوا۟ بِهِۦ كَیۡدࣰا فَجَعَلۡنَـٰهُمُ ٱلۡأَسۡفَلِینَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 99,
      "text": "وَقَالَ إِنِّی ذَاهِبٌ إِلَىٰ رَبِّی سَیَهۡدِینِ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 100,
      "text": "رَبِّ هَبۡ لِی مِنَ ٱلصَّـٰلِحِینَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 101,
      "text": "فَبَشَّرۡنَـٰهُ بِغُلَـٰمٍ حَلِیمࣲ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 102,
      "text": "فَلَمَّا بَلَغَ مَعَهُ ٱلسَّعۡیَ قَالَ یَـٰبُنَیَّ إِنِّیۤ أَرَىٰ فِی ٱلۡمَنَامِ أَنِّیۤ أَذۡبَحُكَ فَٱنظُرۡ مَاذَا تَرَىٰۚ قَالَ یَـٰۤأَبَتِ ٱفۡعَلۡ مَا تُؤۡمَرُۖ سَتَجِدُنِیۤ إِن شَاۤءَ ٱللَّهُ مِنَ ٱلصَّـٰبِرِینَ",
      "juz": 23,
      "page": 449
    },
    {
      "number": 103,
      "text": "فَلَمَّاۤ أَسۡلَمَا وَتَلَّهُۥ لِلۡجَبِینِ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 104,
      "text": "وَنَـٰدَیۡنَـٰهُ أَن یَـٰۤإِبۡرَ ٰ⁠هِیمُ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 105,
      "text": "قَدۡ صَدَّقۡتَ ٱلرُّءۡیَاۤۚ إِنَّا كَذَ ٰ⁠لِكَ نَجۡزِی ٱلۡمُحۡسِنِینَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 106,
      "text": "إِنَّ هَـٰذَا لَهُوَ ٱلۡبَلَـٰۤؤُا۟ ٱلۡمُبِینُ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 107,
      "text": "وَفَدَیۡنَـٰهُ بِذِبۡحٍ عَظِیمࣲ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 108,
      "text": "وَتَرَكۡنَا عَلَیۡهِ فِی ٱلۡـَٔاخِرِینَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 109,
      "text": "سَلَـٰمٌ عَلَىٰۤ إِبۡرَ ٰ⁠هِیمَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 110,
      "text": "كَذَ ٰ⁠لِكَ نَجۡزِی ٱلۡمُحۡسِنِینَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 111,
      "text": "إِنَّهُۥ مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِینَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 112,
      "text": "وَبَشَّرۡنَـٰهُ بِإِسۡحَـٰقَ نَبِیࣰّا مِّنَ ٱلصَّـٰلِحِینَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 113,
      "text": "وَبَـٰرَكۡنَا عَلَیۡهِ وَعَلَىٰۤ إِسۡحَـٰقَۚ وَمِن ذُرِّیَّتِهِمَا مُحۡسِنࣱ وَظَالِمࣱ لِّنَفۡسِهِۦ مُبِینࣱ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 114,
      "text": "وَلَقَدۡ مَنَنَّا عَلَىٰ مُوسَىٰ وَهَـٰرُونَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 115,
      "text": "وَنَجَّیۡنَـٰهُمَا وَقَوۡمَهُمَا مِنَ ٱلۡكَرۡبِ ٱلۡعَظِیمِ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 116,
      "text": "وَنَصَرۡنَـٰهُمۡ فَكَانُوا۟ هُمُ ٱلۡغَـٰلِبِینَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 117,
      "text": "وَءَاتَیۡنَـٰهُمَا ٱلۡكِتَـٰبَ ٱلۡمُسۡتَبِینَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 118,
      "text": "وَهَدَیۡنَـٰهُمَا ٱلصِّرَ ٰ⁠طَ ٱلۡمُسۡتَقِیمَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 119,
      "text": "وَتَرَكۡنَا عَلَیۡهِمَا فِی ٱلۡـَٔاخِرِینَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 120,
      "text": "سَلَـٰمٌ عَلَىٰ مُوسَىٰ وَهَـٰرُونَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 121,
      "text": "إِنَّا كَذَ ٰ⁠لِكَ نَجۡزِی ٱلۡمُحۡسِنِینَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 122,
      "text": "إِنَّهُمَا مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِینَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 123,
      "text": "وَإِنَّ إِلۡیَاسَ لَمِنَ ٱلۡمُرۡسَلِینَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 124,
      "text": "إِذۡ قَالَ لِقَوۡمِهِۦۤ أَلَا تَتَّقُونَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 125,
      "text": "أَتَدۡعُونَ بَعۡلࣰا وَتَذَرُونَ أَحۡسَنَ ٱلۡخَـٰلِقِینَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 126,
      "text": "ٱللَّهَ رَبَّكُمۡ وَرَبَّ ءَابَاۤىِٕكُمُ ٱلۡأَوَّلِینَ",
      "juz": 23,
      "page": 450
    },
    {
      "number": 127,
      "text": "فَكَذَّبُوهُ فَإِنَّهُمۡ لَمُحۡضَرُونَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 128,
      "text": "إِلَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِینَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 129,
      "text": "وَتَرَكۡنَا عَلَیۡهِ فِی ٱلۡـَٔاخِرِینَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 130,
      "text": "سَلَـٰمٌ عَلَىٰۤ إِلۡ یَاسِینَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 131,
      "text": "إِنَّا كَذَ ٰ⁠لِكَ نَجۡزِی ٱلۡمُحۡسِنِینَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 132,
      "text": "إِنَّهُۥ مِنۡ عِبَادِنَا ٱلۡمُؤۡمِنِینَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 133,
      "text": "وَإِنَّ لُوطࣰا لَّمِنَ ٱلۡمُرۡسَلِینَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 134,
      "text": "إِذۡ نَجَّیۡنَـٰهُ وَأَهۡلَهُۥۤ أَجۡمَعِینَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 135,
      "text": "إِلَّا عَجُوزࣰا فِی ٱلۡغَـٰبِرِینَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 136,
      "text": "ثُمَّ دَمَّرۡنَا ٱلۡـَٔاخَرِینَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 137,
      "text": "وَإِنَّكُمۡ لَتَمُرُّونَ عَلَیۡهِم مُّصۡبِحِینَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 138,
      "text": "وَبِٱلَّیۡلِۚ أَفَلَا تَعۡقِلُونَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 139,
      "text": "وَإِنَّ یُونُسَ لَمِنَ ٱلۡمُرۡسَلِینَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 140,
      "text": "إِذۡ أَبَقَ إِلَى ٱلۡفُلۡكِ ٱلۡمَشۡحُونِ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 141,
      "text": "فَسَاهَمَ فَكَانَ مِنَ ٱلۡمُدۡحَضِینَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 142,
      "text": "فَٱلۡتَقَمَهُ ٱلۡحُوتُ وَهُوَ مُلِیمࣱ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 143,
      "text": "فَلَوۡلَاۤ أَنَّهُۥ كَانَ مِنَ ٱلۡمُسَبِّحِینَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 144,
      "text": "لَلَبِثَ فِی بَطۡنِهِۦۤ إِلَىٰ یَوۡمِ یُبۡعَثُونَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 145,
      "text": "۞ فَنَبَذۡنَـٰهُ بِٱلۡعَرَاۤءِ وَهُوَ سَقِیمࣱ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 146,
      "text": "وَأَنۢبَتۡنَا عَلَیۡهِ شَجَرَةࣰ مِّن یَقۡطِینࣲ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 147,
      "text": "وَأَرۡسَلۡنَـٰهُ إِلَىٰ مِا۟ئَةِ أَلۡفٍ أَوۡ یَزِیدُونَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 148,
      "text": "فَـَٔامَنُوا۟ فَمَتَّعۡنَـٰهُمۡ إِلَىٰ حِینࣲ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 149,
      "text": "فَٱسۡتَفۡتِهِمۡ أَلِرَبِّكَ ٱلۡبَنَاتُ وَلَهُمُ ٱلۡبَنُونَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 150,
      "text": "أَمۡ خَلَقۡنَا ٱلۡمَلَـٰۤىِٕكَةَ إِنَـٰثࣰا وَهُمۡ شَـٰهِدُونَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 151,
      "text": "أَلَاۤ إِنَّهُم مِّنۡ إِفۡكِهِمۡ لَیَقُولُونَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 152,
      "text": "وَلَدَ ٱللَّهُ وَإِنَّهُمۡ لَكَـٰذِبُونَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 153,
      "text": "أَصۡطَفَى ٱلۡبَنَاتِ عَلَى ٱلۡبَنِینَ",
      "juz": 23,
      "page": 451
    },
    {
      "number": 154,
      "text": "مَا لَكُمۡ كَیۡفَ تَحۡكُمُونَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 155,
      "text": "أَفَلَا تَذَكَّرُونَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 156,
      "text": "أَمۡ لَكُمۡ سُلۡطَـٰنࣱ مُّبِینࣱ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 157,
      "text": "فَأۡتُوا۟ بِكِتَـٰبِكُمۡ إِن كُنتُمۡ صَـٰدِقِینَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 158,
      "text": "وَجَعَلُوا۟ بَیۡنَهُۥ وَبَیۡنَ ٱلۡجِنَّةِ نَسَبࣰاۚ وَلَقَدۡ عَلِمَتِ ٱلۡجِنَّةُ إِنَّهُمۡ لَمُحۡضَرُونَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 159,
      "text": "سُبۡحَـٰنَ ٱللَّهِ عَمَّا یَصِفُونَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 160,
      "text": "إِلَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِینَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 161,
      "text": "فَإِنَّكُمۡ وَمَا تَعۡبُدُونَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 162,
      "text": "مَاۤ أَنتُمۡ عَلَیۡهِ بِفَـٰتِنِینَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 163,
      "text": "إِلَّا مَنۡ هُوَ صَالِ ٱلۡجَحِیمِ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 164,
      "text": "وَمَا مِنَّاۤ إِلَّا لَهُۥ مَقَامࣱ مَّعۡلُومࣱ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 165,
      "text": "وَإِنَّا لَنَحۡنُ ٱلصَّاۤفُّونَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 166,
      "text": "وَإِنَّا لَنَحۡنُ ٱلۡمُسَبِّحُونَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 167,
      "text": "وَإِن كَانُوا۟ لَیَقُولُونَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 168,
      "text": "لَوۡ أَنَّ عِندَنَا ذِكۡرࣰا مِّنَ ٱلۡأَوَّلِینَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 169,
      "text": "لَكُنَّا عِبَادَ ٱللَّهِ ٱلۡمُخۡلَصِینَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 170,
      "text": "فَكَفَرُوا۟ بِهِۦۖ فَسَوۡفَ یَعۡلَمُونَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 171,
      "text": "وَلَقَدۡ سَبَقَتۡ كَلِمَتُنَا لِعِبَادِنَا ٱلۡمُرۡسَلِینَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 172,
      "text": "إِنَّهُمۡ لَهُمُ ٱلۡمَنصُورُونَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 173,
      "text": "وَإِنَّ جُندَنَا لَهُمُ ٱلۡغَـٰلِبُونَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 174,
      "text": "فَتَوَلَّ عَنۡهُمۡ حَتَّىٰ حِینࣲ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 175,
      "text": "وَأَبۡصِرۡهُمۡ فَسَوۡفَ یُبۡصِرُونَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 176,
      "text": "أَفَبِعَذَابِنَا یَسۡتَعۡجِلُونَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 177,
      "text": "فَإِذَا نَزَلَ بِسَاحَتِهِمۡ فَسَاۤءَ صَبَاحُ ٱلۡمُنذَرِینَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 178,
      "text": "وَتَوَلَّ عَنۡهُمۡ حَتَّىٰ حِینࣲ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 179,
      "text": "وَأَبۡصِرۡ فَسَوۡفَ یُبۡصِرُونَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 180,
      "text": "سُبۡحَـٰنَ رَبِّكَ رَبِّ ٱلۡعِزَّةِ عَمَّا یَصِفُونَ",
      "juz": 23,
      "page": 452
    },
    {
      "number": 181,
      "text": "وَسَلَـٰمٌ عَلَى ٱلۡمُرۡسَلِینَ",
