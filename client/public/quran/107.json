{
  "number": 107,
  "name": "سُورَةُ المَاعُونِ",
  "englishName": "Al-Maa'un",
  "englishNameTranslation": "Almsgiving",
  "revelationType": "Meccan",
  "numberOfAyahs": 7,
  "ayahs": [
    {
      "number": 1,
      "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ أَرَءَیۡتَ ٱلَّذِی یُكَذِّبُ بِٱلدِّینِ",
      "juz": 30,
      "page": 602
    },
    {
      "number": 2,
      "text": "فَذَ ٰ⁠لِكَ ٱلَّذِی یَدُعُّ ٱلۡیَتِیمَ",
      "juz": 30,
      "page": 602
    },
    {
      "number": 3,
      "text": "وَلَا یَحُضُّ عَلَىٰ طَعَامِ ٱلۡمِسۡكِینِ",
      "juz": 30,
      "page": 602
    },
    {
      "number": 4,
      "text": "فَوَیۡلࣱ لِّلۡمُصَلِّینَ",
      "juz": 30,
      "page": 602
    },
    {
      "number": 5,
      "text": "ٱلَّذِینَ هُمۡ عَن صَلَاتِهِمۡ سَاهُونَ",
      "juz": 30,
      "page": 602
    },
    {
      "number": 6,
      "text": "ٱلَّذِینَ هُمۡ یُرَاۤءُونَ",
      "juz": 30,
      "page": 602
    },
    {
      "number": 7,
      "text": "وَیَمۡنَعُونَ ٱلۡمَاعُونَ",
      "juz": 30,
      "page": 602
    }
  ]
}