{
  "number": 99,
  "name": "سُورَةُ الزَّلۡزَلَةِ",
  "englishName": "Az-Zalzala",
  "englishNameTranslation": "The Earthquake",
  "revelationType": "Medinan",
  "numberOfAyahs": 8,
  "ayahs": [
    {
      "number": 1,
      "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ إِذَا زُلۡزِلَتِ ٱلۡأَرۡضُ زِلۡزَالَهَا",
      "juz": 30,
      "page": 599
    },
    {
      "number": 2,
      "text": "وَأَخۡرَجَتِ ٱلۡأَرۡضُ أَثۡقَالَهَا",
      "juz": 30,
      "page": 599
    },
    {
      "number": 3,
      "text": "وَقَالَ ٱلۡإِنسَـٰنُ مَا لَهَا",
      "juz": 30,
      "page": 599
    },
    {
      "number": 4,
      "text": "یَوۡمَىِٕذࣲ تُحَدِّثُ أَخۡبَارَهَا",
      "juz": 30,
      "page": 599
    },
    {
      "number": 5,
      "text": "بِأَنَّ رَبَّكَ أَوۡحَىٰ لَهَا",
      "juz": 30,
      "page": 599
    },
    {
      "number": 6,
      "text": "یَوۡمَىِٕذࣲ یَصۡدُرُ ٱلنَّاسُ أَشۡتَاتࣰا لِّیُرَوۡا۟ أَعۡمَـٰلَهُمۡ",
      "juz": 30,
      "page": 599
    },
    {
      "number": 7,
      "text": "فَمَن یَعۡمَلۡ مِثۡقَالَ ذَرَّةٍ خَیۡرࣰا یَرَهُۥ",
      "juz": 30,
      "page": 599
    },
    {
      "number": 8,
      "text": "وَمَن یَعۡمَلۡ مِثۡقَالَ ذَرَّةࣲ شَرࣰّا یَرَهُۥ",
      "juz": 30,
      "page": 599
    }
  ]
}