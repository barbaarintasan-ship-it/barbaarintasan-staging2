{
  "number": 101,
  "name": "سُورَةُ القَارِعَةِ",
  "englishName": "Al-Qaari'a",
  "englishNameTranslation": "The Calamity",
  "revelationType": "Meccan",
  "numberOfAyahs": 11,
  "ayahs": [
    {
      "number": 1,
      "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ ٱلۡقَارِعَةُ",
      "juz": 30,
      "page": 600
    },
    {
      "number": 2,
      "text": "مَا ٱلۡقَارِعَةُ",
      "juz": 30,
      "page": 600
    },
    {
      "number": 3,
      "text": "وَمَاۤ أَدۡرَىٰكَ مَا ٱلۡقَارِعَةُ",
      "juz": 30,
      "page": 600
    },
    {
      "number": 4,
      "text": "یَوۡمَ یَكُونُ ٱلنَّاسُ كَٱلۡفَرَاشِ ٱلۡمَبۡثُوثِ",
      "juz": 30,
      "page": 600
    },
    {
      "number": 5,
      "text": "وَتَكُونُ ٱلۡجِبَالُ كَٱلۡعِهۡنِ ٱلۡمَنفُوشِ",
      "juz": 30,
      "page": 600
    },
    {
      "number": 6,
      "text": "فَأَمَّا مَن ثَقُلَتۡ مَوَ ٰ⁠زِینُهُۥ",
      "juz": 30,
      "page": 600
    },
    {
      "number": 7,
      "text": "فَهُوَ فِی عِیشَةࣲ رَّاضِیَةࣲ",
      "juz": 30,
      "page": 600
    },
    {
      "number": 8,
      "text": "وَأَمَّا مَنۡ خَفَّتۡ مَوَ ٰ⁠زِینُهُۥ",
      "juz": 30,
      "page": 600
    },
    {
      "number": 9,
      "text": "فَأُمُّهُۥ هَاوِیَةࣱ",
      "juz": 30,
      "page": 600
    },
    {
      "number": 10,
      "text": "وَمَاۤ أَدۡرَىٰكَ مَا هِیَهۡ",
      "juz": 30,
      "page": 600
    },
    {
      "number": 11,
      "text": "نَارٌ حَامِیَةُۢ",
      "juz": 30,
      "page": 600
    }
  ]
}