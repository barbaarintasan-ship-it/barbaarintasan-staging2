{
  "number": 77,
  "name": "سُورَةُ المُرۡسَلَاتِ",
  "englishName": "Al-Mursalaat",
  "englishNameTranslation": "The Emissaries",
  "revelationType": "Meccan",
  "numberOfAyahs": 50,
  "ayahs": [
    {
      "number": 1,
      "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ وَٱلۡمُرۡسَلَـٰتِ عُرۡفࣰا",
      "juz": 29,
      "page": 580
    },
    {
      "number": 2,
      "text": "فَٱلۡعَـٰصِفَـٰتِ عَصۡفࣰا",
      "juz": 29,
      "page": 580
    },
    {
      "number": 3,
      "text": "وَٱلنَّـٰشِرَ ٰ⁠تِ نَشۡرࣰا",
      "juz": 29,
      "page": 580
    },
    {
      "number": 4,
      "text": "فَٱلۡفَـٰرِقَـٰتِ فَرۡقࣰا",
      "juz": 29,
      "page": 580
    },
    {
      "number": 5,
      "text": "فَٱلۡمُلۡقِیَـٰتِ ذِكۡرًا",
      "juz": 29,
      "page": 580
    },
    {
      "number": 6,
      "text": "عُذۡرًا أَوۡ نُذۡرًا",
      "juz": 29,
      "page": 580
    },
    {
      "number": 7,
      "text": "إِنَّمَا تُوعَدُونَ لَوَ ٰ⁠قِعࣱ",
      "juz": 29,
      "page": 580
    },
    {
      "number": 8,
      "text": "فَإِذَا ٱلنُّجُومُ طُمِسَتۡ",
      "juz": 29,
      "page": 580
    },
    {
      "number": 9,
      "text": "وَإِذَا ٱلسَّمَاۤءُ فُرِجَتۡ",
      "juz": 29,
      "page": 580
    },
    {
      "number": 10,
      "text": "وَإِذَا ٱلۡجِبَالُ نُسِفَتۡ",
      "juz": 29,
      "page": 580
    },
    {
      "number": 11,
      "text": "وَإِذَا ٱلرُّسُلُ أُقِّتَتۡ",
      "juz": 29,
      "page": 580
    },
    {
      "number": 12,
      "text": "لِأَیِّ یَوۡمٍ أُجِّلَتۡ",
      "juz": 29,
      "page": 580
    },
    {
      "number": 13,
      "text": "لِیَوۡمِ ٱلۡفَصۡلِ",
      "juz": 29,
      "page": 580
    },
    {
      "number": 14,
      "text": "وَمَاۤ أَدۡرَىٰكَ مَا یَوۡمُ ٱلۡفَصۡلِ",
      "juz": 29,
      "page": 580
    },
    {
      "number": 15,
      "text": "وَیۡلࣱ یَوۡمَىِٕذࣲ لِّلۡمُكَذِّبِینَ",
      "juz": 29,
      "page": 580
    },
    {
      "number": 16,
      "text": "أَلَمۡ نُهۡلِكِ ٱلۡأَوَّلِینَ",
      "juz": 29,
      "page": 580
    },
    {
      "number": 17,
      "text": "ثُمَّ نُتۡبِعُهُمُ ٱلۡـَٔاخِرِینَ",
      "juz": 29,
      "page": 580
    },
    {
      "number": 18,
      "text": "كَذَ ٰ⁠لِكَ نَفۡعَلُ بِٱلۡمُجۡرِمِینَ",
      "juz": 29,
      "page": 580
    },
    {
      "number": 19,
      "text": "وَیۡلࣱ یَوۡمَىِٕذࣲ لِّلۡمُكَذِّبِینَ",
      "juz": 29,
      "page": 580
    },
    {
      "number": 20,
      "text": "أَلَمۡ نَخۡلُقكُّم مِّن مَّاۤءࣲ مَّهِینࣲ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 21,
      "text": "فَجَعَلۡنَـٰهُ فِی قَرَارࣲ مَّكِینٍ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 22,
      "text": "إِلَىٰ قَدَرࣲ مَّعۡلُومࣲ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 23,
      "text": "فَقَدَرۡنَا فَنِعۡمَ ٱلۡقَـٰدِرُونَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 24,
      "text": "وَیۡلࣱ یَوۡمَىِٕذࣲ لِّلۡمُكَذِّبِینَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 25,
      "text": "أَلَمۡ نَجۡعَلِ ٱلۡأَرۡضَ كِفَاتًا",
      "juz": 29,
      "page": 581
    },
    {
      "number": 26,
      "text": "أَحۡیَاۤءࣰ وَأَمۡوَ ٰ⁠تࣰا",
      "juz": 29,
      "page": 581
    },
    {
      "number": 27,
      "text": "وَجَعَلۡنَا فِیهَا رَوَ ٰ⁠سِیَ شَـٰمِخَـٰتࣲ وَأَسۡقَیۡنَـٰكُم مَّاۤءࣰ فُرَاتࣰا",
      "juz": 29,
      "page": 581
    },
    {
      "number": 28,
      "text": "وَیۡلࣱ یَوۡمَىِٕذࣲ لِّلۡمُكَذِّبِینَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 29,
      "text": "ٱنطَلِقُوۤا۟ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 30,
      "text": "ٱنطَلِقُوۤا۟ إِلَىٰ ظِلࣲّ ذِی ثَلَـٰثِ شُعَبࣲ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 31,
      "text": "لَّا ظَلِیلࣲ وَلَا یُغۡنِی مِنَ ٱللَّهَبِ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 32,
      "text": "إِنَّهَا تَرۡمِی بِشَرَرࣲ كَٱلۡقَصۡرِ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 33,
      "text": "كَأَنَّهُۥ جِمَـٰلَتࣱ صُفۡرࣱ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 34,
      "text": "وَیۡلࣱ یَوۡمَىِٕذࣲ لِّلۡمُكَذِّبِینَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 35,
      "text": "هَـٰذَا یَوۡمُ لَا یَنطِقُونَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 36,
      "text": "وَلَا یُؤۡذَنُ لَهُمۡ فَیَعۡتَذِرُونَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 37,
      "text": "وَیۡلࣱ یَوۡمَىِٕذࣲ لِّلۡمُكَذِّبِینَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 38,
      "text": "هَـٰذَا یَوۡمُ ٱلۡفَصۡلِۖ جَمَعۡنَـٰكُمۡ وَٱلۡأَوَّلِینَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 39,
      "text": "فَإِن كَانَ لَكُمۡ كَیۡدࣱ فَكِیدُونِ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 40,
      "text": "وَیۡلࣱ یَوۡمَىِٕذࣲ لِّلۡمُكَذِّبِینَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 41,
      "text": "إِنَّ ٱلۡمُتَّقِینَ فِی ظِلَـٰلࣲ وَعُیُونࣲ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 42,
      "text": "وَفَوَ ٰ⁠كِهَ مِمَّا یَشۡتَهُونَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 43,
      "text": "كُلُوا۟ وَٱشۡرَبُوا۟ هَنِیۤـَٔۢا بِمَا كُنتُمۡ تَعۡمَلُونَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 44,
      "text": "إِنَّا كَذَ ٰ⁠لِكَ نَجۡزِی ٱلۡمُحۡسِنِینَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 45,
      "text": "وَیۡلࣱ یَوۡمَىِٕذࣲ لِّلۡمُكَذِّبِینَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 46,
      "text": "كُلُوا۟ وَتَمَتَّعُوا۟ قَلِیلًا إِنَّكُم مُّجۡرِمُونَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 47,
      "text": "وَیۡلࣱ یَوۡمَىِٕذࣲ لِّلۡمُكَذِّبِینَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 48,
      "text": "وَإِذَا قِیلَ لَهُمُ ٱرۡكَعُوا۟ لَا یَرۡكَعُونَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 49,
      "text": "وَیۡلࣱ یَوۡمَىِٕذࣲ لِّلۡمُكَذِّبِینَ",
      "juz": 29,
      "page": 581
    },
    {
      "number": 50,
      "text": "فَبِأَیِّ حَدِیثِۭ بَعۡدَهُۥ یُؤۡمِنُونَ",
      "juz": 29,
      "page": 581
    }
  ]
}