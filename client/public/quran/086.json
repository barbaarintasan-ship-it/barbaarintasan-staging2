{
  "number": 86,
  "name": "سُورَةُ الطَّارِقِ",
  "englishName": "At-Taariq",
  "englishNameTranslation": "The Morning Star",
  "revelationType": "Meccan",
  "numberOfAyahs": 17,
  "ayahs": [
    {
      "number": 1,
      "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ وَٱلسَّمَاۤءِ وَٱلطَّارِقِ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 2,
      "text": "وَمَاۤ أَدۡرَىٰكَ مَا ٱلطَّارِقُ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 3,
      "text": "ٱلنَّجۡمُ ٱلثَّاقِبُ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 4,
      "text": "إِن كُلُّ نَفۡسࣲ لَّمَّا عَلَیۡهَا حَافِظࣱ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 5,
      "text": "فَلۡیَنظُرِ ٱلۡإِنسَـٰنُ مِمَّ خُلِقَ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 6,
      "text": "خُلِقَ مِن مَّاۤءࣲ دَافِقࣲ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 7,
      "text": "یَخۡرُجُ مِنۢ بَیۡنِ ٱلصُّلۡبِ وَٱلتَّرَاۤىِٕبِ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 8,
      "text": "إِنَّهُۥ عَلَىٰ رَجۡعِهِۦ لَقَادِرࣱ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 9,
      "text": "یَوۡمَ تُبۡلَى ٱلسَّرَاۤىِٕرُ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 10,
      "text": "فَمَا لَهُۥ مِن قُوَّةࣲ وَلَا نَاصِرࣲ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 11,
      "text": "وَٱلسَّمَاۤءِ ذَاتِ ٱلرَّجۡعِ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 12,
      "text": "وَٱلۡأَرۡضِ ذَاتِ ٱلصَّدۡعِ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 13,
      "text": "إِنَّهُۥ لَقَوۡلࣱ فَصۡلࣱ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 14,
      "text": "وَمَا هُوَ بِٱلۡهَزۡلِ",
      "juz": 30,
      "page": 591
    },
    {
      "number": 15,
      "text": "إِنَّهُمۡ یَكِیدُونَ كَیۡدࣰا",
      "juz": 30,
      "page": 591
    },
    {
      "number": 16,
      "text": "وَأَكِیدُ كَیۡدࣰا",
      "juz": 30,
      "page": 591
    },
    {
      "number": 17,
      "text": "فَمَهِّلِ ٱلۡكَـٰفِرِینَ أَمۡهِلۡهُمۡ رُوَیۡدَۢا",
      "juz": 30,
      "page": 591
    }
  ]
}