{
  "number": 55,
  "name": "سُورَةُ الرَّحۡمَٰن",
  "englishName": "Ar-Rahmaan",
  "englishNameTranslation": "The Beneficent",
  "revelationType": "Medinan",
  "numberOfAyahs": 78,
  "ayahs": [
    {
      "number": 1,
      "text": "بِسۡمِ ٱللَّهِ ٱلرَّحۡمَـٰنِ ٱلرَّحِیمِ ٱلرَّحۡمَـٰنُ",
      "juz": 27,
      "page": 531
    },
    {
      "number": 2,
      "text": "عَلَّمَ ٱلۡقُرۡءَانَ",
      "juz": 27,
      "page": 531
    },
    {
      "number": 3,
      "text": "خَلَقَ ٱلۡإِنسَـٰنَ",
      "juz": 27,
      "page": 531
    },
    {
      "number": 4,
      "text": "عَلَّمَهُ ٱلۡبَیَانَ",
      "juz": 27,
      "page": 531
    },
    {
      "number": 5,
      "text": "ٱلشَّمۡسُ وَٱلۡقَمَرُ بِحُسۡبَانࣲ",
      "juz": 27,
      "page": 531
    },
    {
      "number": 6,
      "text": "وَٱلنَّجۡمُ وَٱلشَّجَرُ یَسۡجُدَانِ",
      "juz": 27,
      "page": 531
    },
    {
      "number": 7,
      "text": "وَٱلسَّمَاۤءَ رَفَعَهَا وَوَضَعَ ٱلۡمِیزَانَ",
      "juz": 27,
      "page": 531
    },
    {
      "number": 8,
      "text": "أَلَّا تَطۡغَوۡا۟ فِی ٱلۡمِیزَانِ",
      "juz": 27,
      "page": 531
    },
    {
      "number": 9,
      "text": "وَأَقِیمُوا۟ ٱلۡوَزۡنَ بِٱلۡقِسۡطِ وَلَا تُخۡسِرُوا۟ ٱلۡمِیزَانَ",
      "juz": 27,
      "page": 531
    },
    {
      "number": 10,
      "text": "وَٱلۡأَرۡضَ وَضَعَهَا لِلۡأَنَامِ",
      "juz": 27,
      "page": 531
    },
    {
      "number": 11,
      "text": "فِیهَا فَـٰكِهَةࣱ وَٱلنَّخۡلُ ذَاتُ ٱلۡأَكۡمَامِ",
      "juz": 27,
      "page": 531
    },
    {
      "number": 12,
      "text": "وَٱلۡحَبُّ ذُو ٱلۡعَصۡفِ وَٱلرَّیۡحَانُ",
      "juz": 27,
      "page": 531
    },
    {
      "number": 13,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 531
    },
    {
      "number": 14,
      "text": "خَلَقَ ٱلۡإِنسَـٰنَ مِن صَلۡصَـٰلࣲ كَٱلۡفَخَّارِ",
      "juz": 27,
      "page": 531
    },
    {
      "number": 15,
      "text": "وَخَلَقَ ٱلۡجَاۤنَّ مِن مَّارِجࣲ مِّن نَّارࣲ",
      "juz": 27,
      "page": 531
    },
    {
      "number": 16,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 531
    },
    {
      "number": 17,
      "text": "رَبُّ ٱلۡمَشۡرِقَیۡنِ وَرَبُّ ٱلۡمَغۡرِبَیۡنِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 18,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 19,
      "text": "مَرَجَ ٱلۡبَحۡرَیۡنِ یَلۡتَقِیَانِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 20,
      "text": "بَیۡنَهُمَا بَرۡزَخࣱ لَّا یَبۡغِیَانِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 21,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 22,
      "text": "یَخۡرُجُ مِنۡهُمَا ٱللُّؤۡلُؤُ وَٱلۡمَرۡجَانُ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 23,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 24,
      "text": "وَلَهُ ٱلۡجَوَارِ ٱلۡمُنشَـَٔاتُ فِی ٱلۡبَحۡرِ كَٱلۡأَعۡلَـٰمِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 25,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 26,
      "text": "كُلُّ مَنۡ عَلَیۡهَا فَانࣲ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 27,
      "text": "وَیَبۡقَىٰ وَجۡهُ رَبِّكَ ذُو ٱلۡجَلَـٰلِ وَٱلۡإِكۡرَامِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 28,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 29,
      "text": "یَسۡـَٔلُهُۥ مَن فِی ٱلسَّمَـٰوَ ٰ⁠تِ وَٱلۡأَرۡضِۚ كُلَّ یَوۡمٍ هُوَ فِی شَأۡنࣲ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 30,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 31,
      "text": "سَنَفۡرُغُ لَكُمۡ أَیُّهَ ٱلثَّقَلَانِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 32,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 33,
      "text": "یَـٰمَعۡشَرَ ٱلۡجِنِّ وَٱلۡإِنسِ إِنِ ٱسۡتَطَعۡتُمۡ أَن تَنفُذُوا۟ مِنۡ أَقۡطَارِ ٱلسَّمَـٰوَ ٰ⁠تِ وَٱلۡأَرۡضِ فَٱنفُذُوا۟ۚ لَا تَنفُذُونَ إِلَّا بِسُلۡطَـٰنࣲ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 34,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 35,
      "text": "یُرۡسَلُ عَلَیۡكُمَا شُوَاظࣱ مِّن نَّارࣲ وَنُحَاسࣱ فَلَا تَنتَصِرَانِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 36,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 37,
      "text": "فَإِذَا ٱنشَقَّتِ ٱلسَّمَاۤءُ فَكَانَتۡ وَرۡدَةࣰ كَٱلدِّهَانِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 38,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 39,
      "text": "فَیَوۡمَىِٕذࣲ لَّا یُسۡـَٔلُ عَن ذَنۢبِهِۦۤ إِنسࣱ وَلَا جَاۤنࣱّ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 40,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 532
    },
    {
      "number": 41,
      "text": "یُعۡرَفُ ٱلۡمُجۡرِمُونَ بِسِیمَـٰهُمۡ فَیُؤۡخَذُ بِٱلنَّوَ ٰ⁠صِی وَٱلۡأَقۡدَامِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 42,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 43,
      "text": "هَـٰذِهِۦ جَهَنَّمُ ٱلَّتِی یُكَذِّبُ بِهَا ٱلۡمُجۡرِمُونَ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 44,
      "text": "یَطُوفُونَ بَیۡنَهَا وَبَیۡنَ حَمِیمٍ ءَانࣲ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 45,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 46,
      "text": "وَلِمَنۡ خَافَ مَقَامَ رَبِّهِۦ جَنَّتَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 47,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 48,
      "text": "ذَوَاتَاۤ أَفۡنَانࣲ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 49,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 50,
      "text": "فِیهِمَا عَیۡنَانِ تَجۡرِیَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 51,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 52,
      "text": "فِیهِمَا مِن كُلِّ فَـٰكِهَةࣲ زَوۡجَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 53,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 54,
      "text": "مُتَّكِـِٔینَ عَلَىٰ فُرُشِۭ بَطَاۤىِٕنُهَا مِنۡ إِسۡتَبۡرَقࣲۚ وَجَنَى ٱلۡجَنَّتَیۡنِ دَانࣲ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 55,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 56,
      "text": "فِیهِنَّ قَـٰصِرَ ٰ⁠تُ ٱلطَّرۡفِ لَمۡ یَطۡمِثۡهُنَّ إِنسࣱ قَبۡلَهُمۡ وَلَا جَاۤنࣱّ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 57,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 58,
      "text": "كَأَنَّهُنَّ ٱلۡیَاقُوتُ وَٱلۡمَرۡجَانُ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 59,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 60,
      "text": "هَلۡ جَزَاۤءُ ٱلۡإِحۡسَـٰنِ إِلَّا ٱلۡإِحۡسَـٰنُ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 61,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 62,
      "text": "وَمِن دُونِهِمَا جَنَّتَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 63,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 64,
      "text": "مُدۡهَاۤمَّتَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 65,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 66,
      "text": "فِیهِمَا عَیۡنَانِ نَضَّاخَتَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 67,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 533
    },
    {
      "number": 68,
      "text": "فِیهِمَا فَـٰكِهَةࣱ وَنَخۡلࣱ وَرُمَّانࣱ",
      "juz": 27,
      "page": 534
    },
    {
      "number": 69,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 534
    },
    {
      "number": 70,
      "text": "فِیهِنَّ خَیۡرَ ٰ⁠تٌ حِسَانࣱ",
      "juz": 27,
      "page": 534
    },
    {
      "number": 71,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 534
    },
    {
      "number": 72,
      "text": "حُورࣱ مَّقۡصُورَ ٰ⁠تࣱ فِی ٱلۡخِیَامِ",
      "juz": 27,
      "page": 534
    },
    {
      "number": 73,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 534
    },
    {
      "number": 74,
      "text": "لَمۡ یَطۡمِثۡهُنَّ إِنسࣱ قَبۡلَهُمۡ وَلَا جَاۤنࣱّ",
      "juz": 27,
      "page": 534
    },
    {
      "number": 75,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 534
    },
    {
      "number": 76,
      "text": "مُتَّكِـِٔینَ عَلَىٰ رَفۡرَفٍ خُضۡرࣲ وَعَبۡقَرِیٍّ حِسَانࣲ",
      "juz": 27,
      "page": 534
    },
    {
      "number": 77,
      "text": "فَبِأَیِّ ءَالَاۤءِ رَبِّكُمَا تُكَذِّبَانِ",
      "juz": 27,
      "page": 534
    },
    {
      "number": 78,
      "text": "تَبَـٰرَكَ ٱسۡمُ رَبِّكَ ذِی ٱلۡجَلَـٰلِ وَٱلۡإِكۡرَامِ",
      "juz": 27,
      "page": 534
    }
  ]
}